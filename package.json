{
  "name": "technical-test",
  "version": "0.0.1",
  "description": "A bot that generates motivational messages for your daily life and work.",
  "author": "Axel Morales",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node --max-old-space-size=4096 dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate -d src/shared/database/data-source.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/shared/database/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/shared/database/data-source.ts",
    "migration:show": "npm run typeorm -- migration:show -d src/shared/database/data-source.ts",
    "schema:sync": "npm run typeorm -- schema:sync -d src/shared/database/data-source.ts",
    "schema:drop": "npm run typeorm -- schema:drop -d src/shared/database/data-source.ts"
  },
  "dependencies": {
    "@azure/search-documents": "^12.1.0",
    "@fastify/multipart": "^9.0.3",
    "@fastify/static": "^8.2.0",
    "@langchain/azure-openai": "^0.0.11",
    "@langchain/community": "^0.3.53",
    "@langchain/core": "^0.3.72",
    "@nestjs/common": "^11.1.5 ",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.5 ",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/express": "^5.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "langchain": "^0.3.31",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^5.4.54",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.26"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7 ",
    "@nestjs/testing": "^11.1.5 ",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
